{
    "name": "agentmemory",
    "displayName": "agentMemory - Persistent Memory for AI Agents",
    "description": "Persistent, searchable memory system for AI coding agents. Provides VSCode commands, dashboard analytics, and MCP integration for Cline, RooCode, and other AI assistants.",
    "version": "0.2.0",
    "publisher": "webzler",
    "author": {
        "name": "Amit Rathiesh",
        "email": "amitrathiesh@webzler.com",
        "url": "https://www.webzler.com"
    },
    "icon": "images/icon.png",
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "Other",
        "Machine Learning"
    ],
    "keywords": [
        "ai",
        "agent",
        "memory",
        "mcp",
        "model-context-protocol",
        "cline",
        "roocode",
        "kilocode",
        "context",
        "persistent-memory",
        "ai-assistant"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "agentMemory.openDashboard",
                "title": "Open Dashboard (Webview)",
                "category": "agentMemory",
                "icon": "$(server)"
            },
            {
                "command": "agentMemory.searchMemories",
                "title": "Search Memories",
                "category": "agentMemory",
                "icon": "$(search)"
            },
            {
                "command": "agentMemory.viewAllMemories",
                "title": "View All Memories",
                "category": "agentMemory",
                "icon": "$(list-unordered)"
            },
            {
                "command": "agentMemory.viewByType",
                "title": "View Memories by Type",
                "category": "agentMemory",
                "icon": "$(filter)"
            },
            {
                "command": "agentMemory.clearCache",
                "title": "Clear Memory Cache",
                "category": "agentMemory",
                "icon": "$(trash)"
            },
            {
                "command": "agentMemory.openDashboardBrowser",
                "title": "Open Dashboard in Browser",
                "category": "agentMemory",
                "icon": "$(globe)"
            },
            {
                "command": "agentMemory.showStats",
                "title": "Show Memory Stats",
                "category": "agentMemory"
            },
            {
                "command": "agentMemory.startDashboardServer",
                "title": "Start Dashboard Server (Debug Mode)",
                "category": "agentMemory",
                "icon": "$(server)"
            }
        ],
        "configuration": {
            "title": "agentMemory",
            "properties": {
                "agentMemory.rateLimit.maxRequests": {
                    "type": "number",
                    "default": 100,
                    "minimum": 10,
                    "maximum": 1000,
                    "description": "Maximum API requests per minute per extension"
                },
                "agentMemory.rateLimit.windowMs": {
                    "type": "number",
                    "default": 60000,
                    "minimum": 1000,
                    "maximum": 600000,
                    "description": "Rate limit window in milliseconds"
                },
                "agentMemory.conflictResolution.strategy": {
                    "type": "string",
                    "enum": [
                        "newest-wins",
                        "mcp-wins",
                        "markdown-wins"
                    ],
                    "default": "newest-wins",
                    "enumDescriptions": [
                        "Use the memory with the most recent timestamp",
                        "Always prefer the MCP storage version",
                        "Always prefer the markdown file version"
                    ],
                    "description": "Strategy for resolving memory conflicts between MCP and markdown"
                },
                "agentMemory.sync.autoSync": {
                    "type": "boolean",
                    "default": true,
                    "description": "Automatically sync markdown files to MCP storage when they change"
                },
                "agentMemory.cache.ttl": {
                    "type": "number",
                    "default": 3600000,
                    "minimum": 1000,
                    "maximum": 86400000,
                    "description": "Cache TTL in milliseconds (1 second to 24 hours)"
                },
                "agentMemory.cache.maxSize": {
                    "type": "number",
                    "default": 10000,
                    "minimum": 100,
                    "maximum": 100000,
                    "description": "Maximum number of cached memory entries"
                },
                "agentMemory.dashboard.port": {
                    "type": "number",
                    "default": 3333,
                    "minimum": 1024,
                    "maximum": 65535,
                    "description": "Port number for the dashboard HTTP server"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "package": "vsce package",
        "publish": "vsce publish"
    },
    "devDependencies": {
        "@types/chokidar": "^1.7.5",
        "@types/jest": "^30.0.0",
        "@types/node": "^20.0.0",
        "@types/vscode": "^1.85.0",
        "@vscode/test-electron": "^2.3.0",
        "@vscode/vsce": "^2.22.0",
        "jest": "^30.2.0",
        "ts-jest": "^29.4.6",
        "typescript": "^5.3.0"
    },
    "dependencies": {
        "@types/uuid": "^10.0.0",
        "chokidar": "^5.0.0",
        "fastify": "^4.25.0",
        "keyv": "^4.5.4",
        "keyv-file": "^0.2.0",
        "level": "^8.0.0",
        "lru-cache": "^10.1.0",
        "msgpack-lite": "^0.1.26",
        "uuid": "^13.0.0",
        "zod": "^4.3.5"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/webzler/agentMemory"
    },
    "bugs": {
        "url": "https://github.com/webzler/agentMemory/issues",
        "email": "amitrathiesh@webzler.com"
    },
    "homepage": "https://www.webzler.com",
    "license": "MIT"
}